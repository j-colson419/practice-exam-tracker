        <precision>3</precision>
        <required>false</required>
        <scale>0</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Exam_Type__c</fullName>
        <defaultValue>$CustomMetadata.Exam_Type__mdt.ADM_201.Label</defaultValue>
        <externalId>false</externalId>
        <label>Exam Type</label>
        <required>true</required>
        <trackTrending>false</trackTrending>
        <type>Picklist</type>
        <valueSet>
            <restricted>true</restricted>
            <valueSetName>Exam_Types</valueSetName>
        </valueSet>
    </fields>
    <fields>
        <fullName>Exam_Version__c</fullName>
        <externalId>false</externalId>
        <formula>IF ( RecordType.Name = &#39;Practice&#39;, &#39;N/A&#39;,
IF((MONTH(Attempt_Date__c)  = 2 ||MONTH(Attempt_Date__c)  = 3 ||MONTH(Attempt_Date__c)  = 4 ||MONTH(Attempt_Date__c)  = 5), &quot;Spring &#39;&quot;, (IF ( (MONTH(Attempt_Date__c)  = 6 ||MONTH(Attempt_Date__c)  = 7 ||MONTH(Attempt_Date__c)  = 8 ||MONTH(Attempt_Date__c)  = 9), &quot;Summer &#39;&quot;, &quot;Winter &#39;&quot;))) + RIGHT(TEXT(YEAR( Attempt_Date__c)), 2)
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Exam Version</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Overall_Score__c</fullName>
        <externalId>false</externalId>
        <label>Overall Score</label>
        <precision>5</precision>
        <required>false</required>
        <scale>2</scale>
        <trackTrending>false</trackTrending>
        <type>Percent</type>
    </fields>
    <fields>
        <fullName>Pass__c</fullName>
        <externalId>false</externalId>
        <formula>(TEXT(Exam_Type__c) =  $CustomMetadata.Exam_Type__mdt.ADM_201.MasterLabel 
     &amp;&amp;  Overall_Score__c  &gt;=   $CustomMetadata.Exam_Type__mdt.ADM_201.Passing_Score__c)
||
(TEXT(Exam_Type__c) =  $CustomMetadata.Exam_Type__mdt.PD1.MasterLabel 
     &amp;&amp;  Overall_Score__c  &gt;=   $CustomMetadata.Exam_Type__mdt.PD1.Passing_Score__c)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Pass</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Practice_Exam_Version__c</fullName>
